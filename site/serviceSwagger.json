        "/eportal/eCustomer/api/getCustomerAssociatedServices": {
            "get": {
                "summary": "Get Customer Associated Services",
                "description": "Retrieve the list of services linked to a customer.",
                "parameters": [
                    {
                        "name": "accountNumber",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "format",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "default": "json"
                        }
                    },
                    {
                        "name": "version",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 1
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "example": {
                                    "resultCode": 0,
                                    "resultMessageCode": "acc-ser-001",
                                    "payload": [
                                        {
                                            "accountNumber": "504525168",
                                            "customerName": "John Doe",
                                            "serviceNumber": "0670100549",
                                            "status": "Active"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "400": {
                        "$ref": "#/components/responses/BusinessError"
                    },
                    "500": {
                        "$ref": "#/components/responses/TechnicalError"
                    }
                }
            }
        },
