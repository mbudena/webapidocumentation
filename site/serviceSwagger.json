{
    "openapi": "3.0.0",
    "info": {
        "title": "Eportal API",
        "description": "API documentation for ePortal services.",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "ecdev02selfservice.telkom.co.za"
        }
    ],
    "components": {
        "securitySchemes": {
            "OAuth2": {
                "type": "oauth2",
                "flows": {
                    "password": {
                        "tokenUrl": "https://loginecdev02.telkom.co.za/oic_rest/rest/mobileoamauthentication/acesss",
                        "scopes": {
                            "read": "Access to read data",
                            "write": "Access to write data"
                        }
                    }
                }
            }
        },
        "responses": {
            "NoError": {
                "description": "No error has occurred.",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "example": 0
                                },
                                "message": {
                                    "type": "string",
                                    "example": "Request successful"
                                }
                            }
                        }
                    }
                }
            },
            "BusinessError": {
                "description": "A business-related error has occurred.",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "example": 1
                                },
                                "message": {
                                    "type": "string",
                                    "example": "A business error has occurred"
                                }
                            }
                        }
                    }
                }
            },
            "TechnicalError": {
                "description": "A technical error has occurred.",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "example": -1
                                },
                                "message": {
                                    "type": "string",
                                    "example": "A technical error has occurred"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "paths": {
        "/oic_rest/rest/mobileoamauthentication/register": {
            "post": {
                "summary": "Register mobile authentication and get a client registration handle",
                "operationId": "registerMobileAuthentication",
                "description": "This endpoint registers the mobile authentication and returns a `CLIENTREGHANDLE`.",
                "parameters": [
                    {
                        "in": "header",
                        "name": "X-IDAAS-SERVICEDOMAIN",
                        "schema": {
                            "type": "string",
                            "example": "MobileServiceDomain"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Request body to register mobile authentication.",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "X-Idaas-Rest-Subject-Type": {
                                        "type": "string",
                                        "example": "USERCREDENTIAL"
                                    },
                                    "X-Idaas-Rest-Subject-Username": {
                                        "type": "string",
                                        "example": "lombarj2@telkom.co.za"
                                    },
                                    "X-Idaas-Rest-Subject-Password": {
                                        "type": "string",
                                        "example": "11111111"
                                    },
                                    "X-Idaas-Rest-New-Token-Type-To-Create": {
                                        "type": "string",
                                        "example": "CLIENTREGHANDLE"
                                    },
                                    "clientId": {
                                        "type": "string",
                                        "example": "Telkom"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response with the registration handle token.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "X-Idaas-Rest-Token-Type": {
                                            "type": "string",
                                            "example": "CLIENTREGHANDLE"
                                        },
                                        "X-Idaas-Rest-Token-Value": {
                                            "type": "string",
                                            "example": ".25RC1IK8sbx+8pOWk7v+uBder0t441KqQ35ch7XU8NE="
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/oic_rest/rest/mobileoamauthentication/authenticate": {
            "post": {
                "summary": "Authenticate a user",
                "description": "Authenticates a user and returns a user token.",
                "parameters": [
                    {
                        "in": "header",
                        "name": "X-IDAAS-SERVICEDOMAIN",
                        "schema": {
                            "type": "string",
                            "example": "MobileServiceDomain"
                        }
                    },
                    {
                        "in": "header",
                        "name": "X-Idaas-Rest-Authorization",
                        "schema": {
                            "type": "string",
                            "example": "UIDPASSWORD cred=\"base64(Idaas-Rest-Token-Value)"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "X-Idaas-Rest-Subject-Type": {
                                        "type": "string",
                                        "example": "USERCREDENTIAL"
                                    },
                                    "X-Idaas-Rest-Subject-Username": {
                                        "type": "string",
                                        "example": "lombarj2@telkom.co.za"
                                    },
                                    "X-Idaas-Rest-Subject-Password": {
                                        "type": "string",
                                        "example": "11111111"
                                    },
                                    "X-Idaas-Rest-New-Token-Type-To-Create": {
                                        "type": "string",
                                        "example": "CLIENTREGHANDLE"
                                    },
                                    "clientId": {
                                        "type": "string",
                                        "example": "Telkom"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Access token generated successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "X-Idaas-Rest-Provider-Type": {
                                            "type": "string",
                                            "example": "OAM_11G"
                                        },
                                        "X-Idaas-Rest-Token-Type": {
                                            "type": "string",
                                            "example": "USERTOKEN"
                                        },
                                        "X-Idaas-Rest-Token-Value": {
                                            "type": "string",
                                            "example": ""
                                        },
                                        "X-Idaas-Rest-User-Principal": {
                                            "type": "string",
                                            "example": "lombarj2@telkom.co.za"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
