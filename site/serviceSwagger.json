                "summary": "Get Free Products",
                "description": "Returns the customers free resources. i.e. free minutes, SMSs, mobile data bundles, Wi-Fi data bundles",
                "parameters": [
                    {
                        "name": "msisdn",
                        "in": "query",
                        "required": true,
                        "description": "MSISDN of either the MSISDN in context or an associated MSISDN",
                        "schema": {
                            "type": "string",
                            "example": "0811111111"
                        }
                    },
                    {
                        "name": "version",
